        </activity>
        <activity android:name="uce.edu.ec.vista.RegistroActivity" />
        <activity android:name="uce.edu.ec.vista.ListaActivity" />
        <activity android:name="uce.edu.ec.vista.RegistroVehiculoActivity"></activity>
